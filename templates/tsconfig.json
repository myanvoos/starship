{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "jsx": "react",
      "jsxFactory": "h",
      "jsxFragmentFactory": "Fragment",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"]
  }
  