{
  "name": "starship-jsx",
  "version": "0.1.4",
  "description": "A novel JSX-based frontend framework",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "create-starship-app": "./src/cli/create-starship-app.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./plugin": {
      "import": "./dist/vite-starship.mjs",
      "require": "./dist/vite-starship.js",
      "types": "./dist/vite-starship.d.ts"
    }
  },
  "files": [
    "dist",
    "src/cli",
    "templates",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "npm run build:types && npm run build:lib",
    "build:types": "tsc",
    "build:lib": "vite build",
    "prepublishOnly": "npm run build && chmod +x src/cli/create-starship-app.js",
    "postinstall": "chmod +x src/cli/create-starship-app.js",
    "start": "vite",
    "preview": "vite preview",
    "test": "npx playwright test",
    "create-starship-app": "node ./src/cli/create-starship-app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mylanvoos/starship.git"
  },
  "keywords": [
    "JSX",
    "reactivity",
    "frontend",
    "framework"
  ],
  "author": "Myan V.",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@types/node": "^22.8.6",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  },
  "dependencies": {
    "acorn": "^8.14.0",
    "acorn-jsx": "^5.3.2",
    "entities": "^5.0.0",
    "prompts": "^2.4.2"
  },
  "peerDependencies": {
    "typescript": ">=4.x"
  }
}
